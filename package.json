{
  "name": "react-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "flow-bin": "0.69.0",
    "prettier": "1.11.1",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-redux": "5.0.7",
    "react-scripts": "1.1.3",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "format": "prettier --write \"src/**/*.{js,jsx}\" --print-width 100 --single-quote --trailing-comma es5 --no-bracket-spacing",
    "format:dry": "prettier -l \"src/**/*.{js,jsx}\" --print-width 100 --single-quote --trailing-comma es5 --no-bracket-spacing"
  }
}
